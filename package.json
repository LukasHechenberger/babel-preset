{
  "name": "@ls-age/babel-preset",
  "version": "0.3.0-beta.0",
  "description": "Babel preset used by ls-age",
  "main": "out",
  "scripts": {
    "compile": "babel -d out src",
    "commitmsg": "commitlint -p @commitlint/config-angular -e $GIT_PARAMS",
    "lint": "eslint src test",
    "test": "ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LukasHechenberger/babel-preset.git"
  },
  "author": "Lukas Hechenberger <npm@ls-age.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LukasHechenberger/babel-preset/issues"
  },
  "homepage": "https://github.com/LukasHechenberger/babel-preset#readme",
  "devDependencies": {
    "@commitlint/cli": "^5.0.1",
    "@commitlint/config-angular": "^4.3.0",
    "ava": "^0.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-register": "^6.26.0",
    "conventional-changelog-cli": "^1.3.5",
    "conventional-github-releaser": "^2.0.0",
    "eslint": "^4.11.0",
    "eslint-config-lsage": "^0.3.0",
    "husky": "^0.14.3",
    "standard-version": "^4.2.0",
    "tap-xunit": "^1.7.0"
  },
  "dependencies": {
    "babel-preset-env": "^1.6.1"
  },
  "eslintConfig": {
    "extends": "lsage"
  },
  "babel": {
    "presets": [
      "./registered"
    ]
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "standard-version": {
    "skip": {
      "changelog": true,
      "tag": true
    },
    "scripts": {
      "postbump": "npx conventional-changelog -r 0 -i CHANGELOG.md --same-file --preset angular"
    }
  }
}
